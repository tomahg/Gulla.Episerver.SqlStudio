<Project Sdk="Microsoft.NET.Sdk.Razor">
	
  <PropertyGroup>
    <OutputType>Library</OutputType>
  </PropertyGroup>
	
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
	<Version>1.6.992</Version>
	<PackageVersion>1.6.992</PackageVersion>
	<AssemblyVersion>1.6.992.0</AssemblyVersion>
	<FileVersion>1.6.992.0</FileVersion>	  
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="modules\_protected\Gulla.Episerver.SqlStudio\Gulla.Episerver.SqlStudio.zip" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="EPiServer.CMS.Core" Version="[12.0.3, 13.0.0)" />
    <PackageReference Include="EPiServer.CMS.UI.Core" Version="[12.0.2, 13.0.0)" />
  </ItemGroup>

  <ItemGroup>
    <Content Include=".\msbuild\CopyModule.targets">
      <Pack>true</Pack>
      <PackagePath>build\net5.0\$(MSBuildProjectName).targets</PackagePath>
    </Content>
  </ItemGroup>	
	
</Project>